<UserControl x:Class="Authentification.field"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Authentification"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" 
             mc:Ignorable="d" 
             Height="auto" Width="auto">
    <StackPanel Orientation="Horizontal" Margin="0 10 0 0">
        <Button  Margin="3 0 0 0" Height="15" Width="15" HorizontalAlignment="Left" VerticalAlignment="Center" Style="{StaticResource MaterialDesignFloatingActionButton}" >
            <materialDesign:PackIcon Kind="Check" Height="10" Width="10"></materialDesign:PackIcon>
        </Button>
        <TextBox Foreground="#FF726565" x:Name="obj_text" Margin="15 0 0 0" BorderThickness="0,0,0,2" Width="350" BorderBrush="#FF707080" VerticalAlignment="Center" ></TextBox>
    </StackPanel>
</UserControl>
